{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "rust-tauri-leptos-playground",
  "version": "0.0.0",
  "identifier": "com.iruka-neko.rust-tauri-leptos-playground",
  "build": {
    "beforeDevCommand": "trunk serve",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "trunk build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "rust-tauri-leptos-playground",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": []
  }
}
